  {% endembed %}

{# hero band #}
{% embed "@bolt/band.twig" with {
  size: "large",
  theme: "dark",
  fullBleed: true,
  background: {
    "opacity": "medium",
    "fill": "color",
    "focalPoint": {
      "vertical": "center",
      "horizontal": "center"
    },
    "contentItems": [
      {
        "pattern": "image",
        "src": "/pattern-lab/images/content/backgrounds/background-city-skyline-with-boken.jpg",
        "lazyload": true
      }
    ]
  }
  } only %}

    {% block band_content  %}
    {% grid "o-bolt-grid--flex o-bolt-grid--large o-bolt-grid--middle o-bolt-grid--matrix" %}
      {% cell 'u-bolt-width-1/1 u-bolt-width-1/1@medium' %}    
        {% include "@bolt/headline.twig" with {
            text: "Press Releases",
            size: "xxxlarge",
            tag: "h1",
        } only %}
      <div class="u-bolt-margin-bottom-medium">
        {% include "@bolt/subheadline.twig" with {
            text: "Newsworthy is an understatement. Our industry-leading solutions help the world's largest companies build applications engineered for the future.e",
            size: "xxlarge",
            tag: "h2",
        } only %}
      </div>

          </li>
        </ul>
      {% endcell %}
    {% endgrid %}


