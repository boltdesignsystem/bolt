<p>An example of a fade--in-up on icons, looped 100 times, triggered on window load</p>

{% for i in 0..100 %}
  {% include "@bolt-elements-icon/icon.twig" with {
    name: 'bell-solid',
    color: 'navy',
    size: 'xlarge',
    attributes: {
      class: [
        'a-bolt-in--fade-up',
        'a-bolt-duration-short',
        'a-bolt-cascade-fast',
      ]
    }
  }%}
{% endfor %}
