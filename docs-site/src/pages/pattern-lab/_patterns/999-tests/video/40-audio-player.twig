{% set main_content %}
  {% set video %}
    <video-js
      data-account="1900410236"
      data-player="O3FkeBiaDz"
      data-embed="default"
      data-video-id="4892122320001"
      controls
      class="c-base-video"></video-js>
  {% endset %}

  {% include '@bolt-elements-ratio/ratio.twig' with {
    content: video,
    ratio: 'wide'
  } only %}

  {% set audio_player %}
    <div style="max-width: 480px;">
      <video-js
        data-account="1900410236"
        data-player="h8qT2MQfn"
        data-video-id="6305155733001"
        data-playback-rates="[0.75,1,1.25,1.5,2]"
        data-embed="default"
        controls=""
        data-application-id=""
        class="c-base-video c-base-video--audio-only"></video-js>
    </div>
  {% endset %}

  {% set white_theme %}
    <div class="t-bolt-white u-bolt-padding-medium">
      {{ audio_player }}
    </div>
  {% endset %}

  {% set black_theme %}
