            {% include "@bolt-components-progress-bar/progress-bar.twig" with {
              value: "80",
            } only %}
          {% endcell %}
        {% endgrid %}
      {% endblock %}
    {% endembed %}
  {% endcell %}

  {% cell "u-bolt-width-12/12 u-bolt-width-4/12@small" %}
