{% extends "@pl/_default-page-template.twig" %}

{% block global_header %}
  {% include '@pl/_main-site-page-header.twig' with { static_header: true } %}
{% endblock %}

{% block main_content %}
  {% embed "@bolt-components-sticky/sticky.twig" %}
    {% block sticky_content %}
      {% set navbar_data = [
        {
          link: {
            content: 'News',
            attributes: {
              href: '#'
            }
          }
        },
        {
          link: {
            content: 'Press Kit & Resources',
            attributes: {
              href: '#'
            }
          }
        },
        {
          link: {
            content: 'Media Contacts',
            attributes: {
              href: '#'
            }
          }
        },
        {
          link: {
            content: 'Another Link',
            attributes: {
              href: '#'
            }
          }
        }
      ] %}

      {% set navbar_items %}
        {% for item in navbar_data %}
          {% include '@bolt-components-navbar/navbar-li.twig' with item only %}
        {% endfor %}
      {% endset %}

      {% set navbar_list %}
        {% include '@bolt-components-navbar/navbar-ul.twig' with { content: navbar_items } only %}
      {% endset %}

      {% include '@bolt-components-navbar/navbar.twig' with {
        theme: 'xdark',
        title: {
          content: 'Newsroom',
          icon: {
            name: 'info-solid'
          }
        },
        links: navbar_list
      } only %}
    {% endblock %}
  {% endembed %}

  {% embed "@bolt-components-band/band.twig" with {
    size: "large",
    theme: "dark",
    full_bleed: true,
    page_title: page_title|default("Insert Page Title Here"),
    background: {
      shape_group: "B"
    }
  } only %}

    {% block band_content  %}
      {% grid "o-bolt-grid--flex o-bolt-grid--large o-bolt-grid--middle o-bolt-grid--matrix" %}
        {% cell 'u-bolt-width-1/1 u-bolt-width-1/1@medium' %}
          {% include "@bolt/headline.twig" with {
            text: page_title,
            size: "xxxlarge",
            tag: "h1",
          } only %}

          <div class="u-bolt-margin-bottom-medium">
            {% include "@bolt/subheadline.twig" with {
              text: "Newsworthy is an understatement. Our industry-leading solutions help the world's largest companies build applications engineered for the future.",
              size: "xxlarge",
              tag: "h2",
            } only %}
          </div>
        {% endcell %}
      {% endgrid %}


      {% grid "o-bolt-grid--flex o-bolt-grid--matrix" %}
        {% cell "u-bolt-width-1/1 u-bolt-width-4/12@medium" %}
          {% include "@bolt-components-card-replacement/card-replacement.twig" with {
            body: {
              eyebrow: "Jan 18 2018",
