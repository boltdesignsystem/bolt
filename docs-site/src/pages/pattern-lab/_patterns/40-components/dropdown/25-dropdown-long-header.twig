      inset: true,
      spacing: "small",
      items: [
        include('@bolt-elements-text-link/text-link.twig', {
          content: "Link 1",
          attributes: {
            href: '#!'
          }
        }),
        include('@bolt-elements-text-link/text-link.twig', {
          content: "Link 2",
          attributes: {
            href: '#!'
          }
        }),
        include('@bolt-elements-text-link/text-link.twig', {
          content: "Link 3",
          attributes: {
            href: '#!'
          }
        }),
      ]
    } only %}
  {% endblock %}
{% endembed %}
