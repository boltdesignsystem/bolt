{% extends '@bolt-components-form/form-element.twig' %}

{% set label %}
  {% include('@bolt-components-form/form-label.twig') with {
    'title': labelTitle,
    'displayType': 'inline-checkbox',
    'attributes': {
      'for': elementId
    }
  } %}
{% endset %}

{% set children %}
  {% include('@bolt-components-form/form-input.twig') with {
    'attributes': {
      'type': 'checkbox',
      'id': elementId
    },
  } %}
{% endset %}
