{# Schema Validation #}
{% set schema = bolt.data.components['@bolt-components-notifications'].schema['notifications-list'] %}
{% if enable_json_schema_validation %}
  {{ validate_data_schema(schema, _self)|raw }}
{% endif %}

{# Variables #}
{% set this = init(schema) %}
{% set attributes = create_attribute(attributes|default({})) %}

{% set classes = [
  'c-bolt-notifications__list',
] %}

{# Template #}
<ul {{ attributes.addClass(classes) }}>
  {# <li class="c-bolt-notifications__list-item c-bolt-notifications__list-item--section-heading">
    <strong>{{ item.heading_divider }}</strong>
  </li> #}
  {{ content }}
</ul>
