                            <bolt-animate in="slide-down" out="slide-up" class="c-pega-wwo__block-contents" initial-appearance="default">
                              <p class="c-pega-wwo__block-body">Frank calls customer service to cancel his card.</p>
                            </bolt-animate>
                          </bolt-animate>
