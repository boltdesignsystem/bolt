                            <div
                              class="c-pega-wwo__icon c-pega-wwo__icon-graph">
                              {% include "@bolt-components-icon/icon.twig" with {
                                name: "chart-bar",
                                size: "xlarge",
                                attributes: {
                                  style: [
                                    "color: #7ACDCD;",
                                    "--bolt-theme-icon: #7ACDCD;",
                                  ]
                                }
                              } only %}
                            </div>
                            <h4 class="c-pega-wwo__block-title">Business
                              Outcome</h4>
                            <p class="c-pega-wwo__block-body">
                            <ul class="c-pega-wwo__block-ul">
                              <li class="c-pega-wwo__block-li">Customer
                                Churn
                              </li>
                              <li class="c-pega-wwo__block-li">Lost Revenue
                              </li>
                              <li class="c-pega-wwo__block-li">Competitor is
                                Stronger
                              </li>
                            </ul>
                            </p>
                          </bolt-animate>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="c-pega-wwo__bg c-pega-wwo__bg-wo"></div>
              </div>
            </div>
