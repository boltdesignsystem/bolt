                            <div class="c-pega-wwo__icon c-pega-wwo__icon-sad-face">
                              {% include "@bolt-components-icon/icon.twig" with {
                                name: "face-sad",
                                size: "xlarge",
                                attributes: {
                                  style: [
                                    "color: #7ACDCD;",
                                    "--bolt-theme-icon: #7ACDCD;",
                                  ]
                                }
                              } only %}
                            </div>
                            <h4 class="c-pega-wwo__block-title">Customer
