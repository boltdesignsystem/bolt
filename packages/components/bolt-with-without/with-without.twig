                            <div class="block--icon-and-title-wrapper">
                              <div class="c-pega-wwo__icon c-pega-wwo__icon-x">
                                {% include "@bolt-components-icon/icon.twig" with {
                                  name: "close-circled",
                                  size: "medium",
                                  style: [
                                    "color: #FFF;",
                                    "--bolt-theme-icon: #FFF;",
                                  ]
                                } only %}
