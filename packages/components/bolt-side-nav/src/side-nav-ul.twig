{# Schema Validation #}
{% set schema = bolt.data.components['@bolt-components-side-nav'].schema['side-nav-ul'] %}
{% if enable_json_schema_validation %}
  {{ validate_data_schema(schema, _self)|raw }}
{% endif %}

{# Variables #}
{% set this = init(schema) %}
{% set attributes = create_attribute(attributes|default({})) %}

{# Template #}
<ul {{ attributes.addClass('c-bolt-side-nav__list') }}>
  {{ content }}
</ul>
